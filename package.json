    "lively.lang": "^0.7.0",
    "lively.notifications": "^0.3.0",
    "lively.resources": "^0.1.2",
    "lively.vm": "^0.7.10",
    "systemjs": "^0.19.38",
    "systemjs-plugin-babel": "0.0.13"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
