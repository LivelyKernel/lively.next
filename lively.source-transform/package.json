{
  "name": "lively.source-transform",
  "version": "0.1.7",
  "description": "EcmaScript 6 classes for live development",
  "main": "dist/lively.source-transform.js",
  "scripts": {
    "test": "mocha-es6 tests/*-test.js",
    "build": "node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.source-transform.git"
  },
  "keywords": [
    "LivelyWeb",
    "JavaScript",
    "Live Programming"
  ],
  "author": "Robert Krahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LivelyKernel/lively.source-transform/issues"
  },
  "homepage": "https://github.com/LivelyKernel/lively.source-transform",
  "dependencies": {
    "lively.ast": "https://github.com/LivelyKernel/lively.ast",
    "lively.lang": "https://github.com/LivelyKernel/lively.lang",
    "lively.classes": "https://github.com/LivelyKernel/lively.classes"
  },
  "devDependencies": {
    "lively.vm": "*",
    "babel-core": "^6.16.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-jsx": "2.0.0",
    "babel-preset-es2015": "^6.16.0",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1"
  },
  "systemjs": {
    "main": "index.js",
    "map": {
      "babel-plugin-transform-jsx": {
        "~node": "https://jspm.dev/babel-plugin-transform-jsx",
        "node": "@empty"
      },
      "babel-catch-binding": {
        "~node": "https://jspm.dev/@babel/plugin-proposal-optional-catch-binding",
        "node": "@empty"
      },
      "babel-import-meta": {
        "node": "@empty",
	"~node": "https://jspm.dev/@babel/plugin-syntax-import-meta"
      }
    }
  }
}
