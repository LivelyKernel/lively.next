{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
                    "lively.lang/array.js"
                    ],
        "includePattern": "\\.js$"
    },
    "sourceType": "module",
    "opts": {
        "recurse": true,
        "destination": "./docs/"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
