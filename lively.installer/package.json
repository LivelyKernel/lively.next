{
  "name": "lively.installer",
  "version": "0.3.0",
  "type": "module",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.installer"
  },
  "scripts": {
    "build": ". ../scripts/lively-next-flatn-env.sh; lively_next_flatn_env $(dirname \"$(pwd)\"); node --no-warnings --experimental-import-meta-resolve --experimental-loader ../flatn/resolver.mjs build.js"
  },
  "systemjs": {
    "map": {
      "child_process": {
        "node": "@node/child_process",
        "~node": "@empty"
      },
      "fs": {
        "node": "@node/fs",
        "~node": "@empty"
      }
    }
  },
  "dependencies": {
    "lively.modules": "*",
    "systemjs": "^0.19.40",
    "flatn": "*",
    "@rollup/plugin-json": "4.1.0",
    "rollup": "^2.70.2",
    "rollup-plugin-polyfill-node": "0.9.0",
    "babel-runtime": "6.20.0",
    "acorn": "8.0.4",
    "@buxlabs/amd-to-es6": "0.16.3",
    "@babel/core": "^7.12.0",
    "@babel/plugin-transform-modules-systemjs": "7.18.0",
    "@babel/plugin-transform-modules-commonjs": "7.14.0"
  }
}
